{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run webhook",
    "webhook": "lt -p 8080 -s efferfrefrs"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.1",
    "concurrently": "^6.4.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "localtunnel": "^2.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.14",
    "nodemon": "^2.0.15",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^6.1.0",
    "sendgrid": "^5.2.3",
    "stripe": "^8.195.0"
  }
}